{
  "name": "hugsyCMS-frontend",
  "version": "0.0.1",
  "description": "a CMS build by Reactjs",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env ENVIRONMENT_MODE=dev env-cmd node index.js",
    "mock": "cross-env env-cmd nodemon src/__mock__/index.js",
    "dev:with-theme": "cross-env WITH_THEME=true yarn dev",
    "start": "cross-env ENVIRONMENT_MODE=production env-cmd node index.js",
    "build": "cross-env WITH_THEME=true webpack --config webpack.product.config.js",
    "build:dll": "cross-env webpack --config webpack.config.dll.js",
    "deploy": "cross-env yarn pull:code && yarn build:dll && yarn install && yarn release",
    "pull:code": "git pull origin dev",
    "test": "cross-env jest",
    "test:with-coverage": "cross-env TEST_COVERAGE=true jest",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "yarn report-version && cross-env DEPLOY=true yarn build",
    "report-version": "bash scripts/release.sh && yarn update-version",
    "update-version": "node scripts/after-release.js && bash scripts/after-release.sh"
  },
  "author": "kavience",
  "license": "MIT",
